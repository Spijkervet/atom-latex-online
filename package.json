{
  "name": "latex-online",
  "main": "./lib/latex-online",
  "author": "Janne Spijkervet <janne.spijkervet@hotmail.com>",
  "version": "1.1.0",
  "description": "LaTex Online atom package",
  "keywords": [
    "latex",
    "atom",
    "online",
    "compiler"
  ],
  "activationCommands": {
    "atom-workspace": "latex-online:fetch"
  },
  "repository": "https://github.com/spijkervet/atom-latex-online.git",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "atom-message-panel": "^1.3.0",
    "jszip": "^3.1.5",
    "opn": "^5.3.0",
    "request": "^2.85.0"
  },
  "configSchema": {
    "server_url": {
      "title": "LaTeX Online server URL",
      "description": "The full url to your TeX server.",
      "type": "string",
      "default": "http://localhost:5000",
      "order": 1
    }
  }
}
